{
  "name": "@agoric/swingset-vat",
  "version": "0.32.2",
  "description": "Vat/Container Launcher",
  "type": "module",
  "main": "src/index.js",
  "engines": {
    "node": ">=14.15.0"
  },
  "bin": {
    "vat": "bin/vat"
  },
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:xs": "SWINGSET_WORKER_TYPE=xs-worker ava",
    "test:xs-worker": "ava test/workers/test-worker.js -m 'xsnap vat manager'",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error lint:*",
    "lint:types": "tsc",
    "lint:eslint": "eslint ."
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.5.0",
    "@types/microtime": "^2.1.0",
    "@types/tmp": "^0.2.0",
    "@types/yargs-parser": "^21.0.0"
  },
  "dependencies": {
    "@agoric/assert": "workspace:*",
    "@agoric/internal": "workspace:*",
    "@agoric/kmarshal": "workspace:*",
    "@agoric/store": "workspace:*",
    "@agoric/swing-store": "workspace:*",
    "@agoric/swingset-liveslots": "workspace:*",
    "@agoric/swingset-xsnap-supervisor": "workspace:*",
    "@agoric/time": "workspace:*",
    "@agoric/vat-data": "workspace:*",
    "@agoric/xsnap": "workspace:*",
    "@agoric/xsnap-lockdown": "workspace:*",
    "@endo/base64": "^1.0.0",
    "@endo/bundle-source": "^3.0.1",
    "@endo/captp": "^4.0.1",
    "@endo/check-bundle": "^1.0.1",
    "@endo/compartment-mapper": "^1.0.1",
    "@endo/eventual-send": "^1.0.1",
    "@endo/far": "^1.0.1",
    "@endo/import-bundle": "^1.0.1",
    "@endo/init": "^1.0.1",
    "@endo/marshal": "^1.0.1",
    "@endo/nat": "^5.0.1",
    "@endo/patterns": "^1.0.1",
    "@endo/promise-kit": "^1.0.1",
    "@endo/ses-ava": "^1.0.1",
    "@endo/stream": "^1.0.1",
    "@endo/zip": "^1.0.0",
    "ansi-styles": "^6.2.1",
    "anylogger": "^0.21.0",
    "better-sqlite3": "^9.1.1",
    "import-meta-resolve": "^2.2.1",
    "microtime": "^3.1.0",
    "semver": "^6.3.0",
    "tmp": "^0.2.1",
    "yargs-parser": "^21.1.1"
  },
  "peerDependencies": {
    "ava": "^5.3.0"
  },
  "files": [
    "bin/vat",
    "src/**/*.js",
    "src/**/*.d.ts",
    "exported.js",
    "tools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/agoric-sdk.git"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric-sdk/issues"
  },
  "homepage": "https://github.com/Agoric/agoric-sdk#readme",
  "directories": {
    "example": "examples"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "20m",
    "workerThreads": false
  },
  "publishConfig": {
    "access": "public"
  },
  "typeCoverage": {
    "atLeast": 75.26
  }
}
