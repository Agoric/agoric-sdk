{
  "name": "@agoric/portfolio-api",
  "version": "0.1.0",
  "description": "API for Portfolio management",
  "type": "module",
  "files": [
    "src",
    "tools"
  ],
  "main": "src/main.js",
  "bin": "./src/cli/bin.js",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 --all $C8_OPTIONS ava",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "yarn run -T run-s --continue-on-error 'lint:*'",
    "lint:eslint": "yarn run -T eslint .",
    "lint:types": "yarn run -T tsc"
  },
  "devDependencies": {
    "@fast-check/ava": "^2.0.1",
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "ts-blank-space": "^0.6.2"
  },
  "dependencies": {
    "@agoric/client-utils": "workspace:*",
    "@agoric/cosmic-proto": "workspace:*",
    "@agoric/ertp": "workspace:*",
    "@agoric/internal": "workspace:*",
    "@agoric/notifier": "workspace:*",
    "@agoric/orchestration": "workspace:*",
    "@agoric/zoe": "workspace:*",
    "@cosmjs/proto-signing": "^0.36.0",
    "@cosmjs/stargate": "^0.36.0",
    "@endo/base64": "^1.0.12",
    "@endo/common": "^1.2.13",
    "@endo/errors": "^1.2.13",
    "@endo/eventual-send": "^1.3.4",
    "@endo/far": "^1.1.14",
    "@endo/init": "^1.1.12",
    "@endo/marshal": "^1.8.0",
    "@endo/nat": "^5.1.3",
    "@endo/pass-style": "^1.6.3",
    "@endo/patterns": "^1.7.0",
    "@endo/promise-kit": "^1.1.13"
  },
  "ava": {
    "extensions": {
      "js": true,
      "ts": "module"
    },
    "files": [
      "test/**/*.test.*"
    ],
    "nodeArguments": [
      "--import=ts-blank-space/register",
      "--no-warnings"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "20m"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^20.9 || ^22.11"
  }
}
