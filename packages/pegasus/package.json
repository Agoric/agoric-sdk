    "import-meta-resolve": "^4.1.0"
  },
  "files": [
    "scripts/",
    "src/",
    "NEWS.md"
  ],
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "10m"
  },
  "publishConfig": {
    "access": "public"
  },
  "typeCoverage": {
    "atLeast": 91.21
  }
