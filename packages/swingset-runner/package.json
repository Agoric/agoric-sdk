{
  "name": "@agoric/swingset-runner",
  "version": "0.20.0",
  "private": true,
  "description": "Application to launch SwingSet instances for development and testing",
  "type": "module",
  "main": "./src/main.js",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "author": "Agoric",
  "license": "Apache-2.0",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint .",
    "ci:autobench": "./autobench.js"
  },
  "dependencies": {
    "@agoric/assert": "^0.5.0",
    "@agoric/ertp": "^0.15.0",
    "@agoric/nat": "^4.1.0",
    "@agoric/stat-logger": "^0.4.27",
    "@agoric/store": "^0.8.0",
    "@agoric/swing-store": "^0.8.0",
    "@agoric/swingset-vat": "^0.29.0",
    "@agoric/vat-data": "^0.4.0",
    "@agoric/zoe": "^0.25.0",
    "@endo/bundle-source": "^2.3.0",
    "@endo/eventual-send": "^0.16.4",
    "@endo/init": "^0.5.48",
    "@endo/marshal": "^0.7.4",
    "expose-gc": "^1.0.0",
    "lmdb": "^2.4.5",
    "n-readlines": "^1.0.1",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "ava": "^4.3.1",
    "c8": "^7.11.0",
    "import-meta-resolve": "^1.1.1"
  },
  "eslintIgnore": [
    "bundle-*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  }
}
