{
  "name": "@aglocal/fast-usdc-deploy",
  "private": true,
  "version": "0.1.1-u20.0",
  "description": "Deployment facilities of the Fast USDC product",
  "type": "module",
  "files": [
    "src",
    "tools"
  ],
  "scripts": {
    "build": "esbuild ../fast-usdc-contract/src/fast-usdc.contract.ts --bundle --platform=neutral --main-fields=main --outfile=dist/fast-usdc.contract.bundle.js --metafile=dist/fast-usdc.contract.meta.json",
    "test": "yarn build && ava",
    "test:c8": "c8 --all $C8_OPTIONS ava",
    "$scripts-note": "fast-usdc skipped in test:xs pending #10847",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error lint:*",
    "lint:types": "tsc",
    "lint:eslint": "eslint ."
  },
  "devDependencies": {
    "@agoric/vats": "^0.16.0-u20.0",
    "ava": "^5.3.0",
    "c8": "^10.1.2",
    "esbuild": "^0.25.2",
    "execa": "9.1.0",
    "ts-blank-space": "^0.4.4"
  },
  "dependencies": {
    "@agoric/client-utils": "^0.2.0-u20.0",
    "@agoric/cosmic-proto": "^0.5.0-u20.0",
    "@agoric/deploy-script-support": "^0.10.4-u20.0",
    "@agoric/ertp": "^0.16.3-u20.0",
    "@agoric/fast-usdc": "^0.2.0-u20.0",
    "@agoric/internal": "^0.4.0-u20.0",
    "@agoric/notifier": "^0.7.0-u20.0",
    "@agoric/orchestration": "^0.2.0-u20.0",
    "@agoric/store": "^0.9.3-u20.0",
    "@agoric/vat-data": "^0.5.3-u20.0",
    "@agoric/vow": "^0.2.0-u20.0",
    "@agoric/zoe": "^0.27.0-u20.0",
    "@endo/errors": "^1.2.10",
    "@endo/eventual-send": "^1.3.1",
    "@endo/far": "^1.1.11",
    "@endo/init": "^1.1.9",
    "@endo/marshal": "^1.6.4",
    "@endo/pass-style": "^1.5.0",
    "@endo/patterns": "^1.5.0",
    "commander": "^12.1.0",
    "ethers": "^6.13.4"
  },
  "ava": {
    "extensions": {
      "js": true,
      "ts": "module"
    },
    "files": [
      "test/**/*.test.*"
    ],
    "nodeArguments": [
      "--import=ts-blank-space/register",
      "--no-warnings"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "20m"
  }
}
