{
  "name": "@agoric/portfolio-ui-playground",
  "version": "0.1.0",
  "description": "Non-production prototype UI for YMax portfolio operations",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "ava"
  },
  "dependencies": {
    "@preact/signals": "^1.2.2",
    "ethers": "^6.8.1",
    "preact": "^10.19.3"
  },
  "devDependencies": {
    "@endo/init": "^1.1.12",
    "@preact/preset-vite": "^2.7.0",
    "ava": "^6.0.1",
    "ts-blank-space": "^0.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "ava": {
    "extensions": {
      "js": true,
      "ts": "module"
    },
    "files": [
      "test/**/*.test.*"
    ],
    "nodeArguments": [
      "--import=ts-blank-space/register",
      "--no-warnings"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "20m"
  }
}
