{
  "name": "@agoric/solo",
  "version": "0.10.3",
  "description": "Agoric's Solo vat runner",
  "type": "module",
  "bin": {
    "ag-solo": "src/entrypoint.js"
  },
  "main": "src/main.js",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error lint:*",
    "lint:types": "tsc",
    "lint:eslint": "eslint ."
  },
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "dependencies": {
    "@agoric/access-token": "workspace:*",
    "@agoric/assert": "workspace:*",
    "@agoric/cache": "workspace:*",
    "@agoric/cosmic-swingset": "workspace:*",
    "@agoric/internal": "workspace:*",
    "@agoric/notifier": "workspace:*",
    "@agoric/spawner": "workspace:*",
    "@agoric/store": "workspace:*",
    "@agoric/swing-store": "workspace:*",
    "@agoric/swingset-vat": "workspace:*",
    "@agoric/telemetry": "workspace:*",
    "@agoric/time": "workspace:*",
    "@agoric/vats": "workspace:*",
    "@agoric/wallet": "workspace:*",
    "@endo/captp": "^4.0.1",
    "@endo/eventual-send": "^1.0.1",
    "@endo/import-bundle": "^1.0.1",
    "@endo/init": "^1.0.1",
    "@endo/marshal": "^1.0.1",
    "@endo/promise-kit": "^1.0.1",
    "anylogger": "^0.21.0",
    "deterministic-json": "^1.0.5",
    "esm": "agoric-labs/esm#Agoric-built",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.6",
    "import-meta-resolve": "^2.2.1",
    "minimist": "^1.2.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "temp": "^0.9.1",
    "tmp": "^0.2.1",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@agoric/ertp": "workspace:*",
    "@endo/bundle-source": "^3.0.1",
    "ava": "^5.3.0",
    "c8": "^7.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "20m",
    "workerThreads": false
  },
  "typeCoverage": {
    "atLeast": 68.59
  }
}
