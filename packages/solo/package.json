{
  "name": "@agoric/solo",
  "version": "0.10.3",
  "description": "Agoric's Solo vat runner",
  "type": "module",
  "bin": {
    "ag-solo": "src/entrypoint.js"
  },
  "main": "src/main.js",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 --all $C8_OPTIONS ava",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "yarn run -T run-s --continue-on-error 'lint:*'",
    "lint:types": "yarn run -T tsc",
    "lint:eslint": "yarn run -T eslint ."
  },
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "dependencies": {
    "@agoric/access-token": "workspace:*",
    "@agoric/cache": "workspace:*",
    "@agoric/cosmic-proto": "workspace:*",
    "@agoric/cosmic-swingset": "workspace:*",
    "@agoric/internal": "workspace:*",
    "@agoric/notifier": "workspace:*",
    "@agoric/spawner": "workspace:*",
    "@agoric/store": "workspace:*",
    "@agoric/swing-store": "workspace:*",
    "@agoric/swingset-vat": "workspace:*",
    "@agoric/telemetry": "workspace:*",
    "@agoric/time": "workspace:*",
    "@agoric/vats": "workspace:*",
    "@agoric/wallet": "workspace:*",
    "@endo/base64": "^1.0.9",
    "@endo/captp": "^4.4.5",
    "@endo/errors": "^1.2.10",
    "@endo/eventual-send": "^1.3.1",
    "@endo/import-bundle": "^1.4.0",
    "@endo/init": "^1.1.9",
    "@endo/marshal": "^1.6.4",
    "@endo/promise-kit": "^1.1.10",
    "anylogger": "^0.21.0",
    "deterministic-json": "^1.0.5",
    "express": "^5.0.1",
    "http-proxy-middleware": "^2.0.6",
    "import-meta-resolve": "^4.1.0",
    "minimist": "^1.2.0",
    "morgan": "^1.10.0",
    "temp": "^0.9.1",
    "tmp": "^0.2.1",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@agoric/ertp": "workspace:*",
    "@endo/bundle-source": "^4.0.0",
    "ava": "^5.3.0",
    "c8": "^10.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^20.9 || ^22.11"
  },
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "20m",
    "workerThreads": false
  },
  "typeCoverage": {
    "atLeast": 73.95
  }
}
