  "dependencies": {
    "@agoric/assert": "^0.4.0",
    "@agoric/ertp": "^0.14.0",
    "@agoric/nat": "^4.1.0",
    "@agoric/notifier": "^0.4.0",
    "@agoric/store": "^0.7.0",
    "@agoric/zoe": "^0.22.0",
    "@endo/eventual-send": "^0.15.3",
    "@endo/marshal": "^0.6.7",
    "@endo/promise-kit": "^0.2.41",
