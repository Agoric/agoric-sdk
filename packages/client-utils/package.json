{
  "name": "@agoric/client-utils",
  "version": "0.1.0",
  "description": "Utilities for building Agoric clients",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "src/main.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -f '*.d.ts*' '*.tsbuildinfo'",
    "test": "ava",
    "test:c8": "c8 --all $C8_OPTIONS ava",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "yarn run -T run-s --continue-on-error 'lint:*'",
    "lint:eslint": "yarn run -T eslint .",
    "lint:types": "yarn run -T tsc"
  },
  "devDependencies": {
    "ava": "^5.3.0",
    "c8": "^10.1.3",
    "ts-blank-space": "^0.6.1"
  },
  "dependencies": {
    "@agoric/casting": "workspace:*",
    "@agoric/cosmic-proto": "workspace:*",
    "@agoric/ertp": "workspace:*",
    "@agoric/internal": "workspace:*",
    "@agoric/smart-wallet": "workspace:*",
    "@agoric/vats": "workspace:*",
    "@cosmjs/stargate": "^0.33.0",
    "@cosmjs/tendermint-rpc": "^0.33.0",
    "@endo/base64": "^1.0.11",
    "@endo/common": "^1.2.12",
    "@endo/errors": "^1.2.12",
    "@endo/marshal": "^1.7.1",
    "@endo/pass-style": "^1.6.2",
    "@endo/patterns": "^1.6.1",
    "@endo/promise-kit": "^1.1.12"
  },
  "ava": {
    "extensions": {
      "js": true,
      "ts": "module"
    },
    "files": [
      "test/**/*.test.*"
    ],
    "nodeArguments": [
      "--import=ts-blank-space/register",
      "--no-warnings"
    ],
    "require": [
      "@endo/init/legacy.js"
    ],
    "timeout": "20m"
  }
}
