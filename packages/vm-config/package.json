{
  "name": "@agoric/vm-config",
  "version": "0.1.0",
  "description": "Config to start an Agoric VM",
  "type": "module",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/agoric-sdk.git"
  },
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 --all ${C8_OPTIONS:-} ava",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "yarn run -T run-s --continue-on-error 'lint:*'",
    "lint:types": "yarn run -T tsc",
    "lint:eslint": "yarn run -T eslint ."
  },
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "dependencies": {
    "@agoric/builders": "workspace:*",
    "@agoric/inter-protocol": "workspace:*",
    "@agoric/smart-wallet": "workspace:*",
    "@agoric/vats": "workspace:*",
    "@agoric/zoe": "workspace:*"
  },
  "devDependencies": {
    "@endo/init": "^1.1.12",
    "ava": "^6.4.1",
    "c8": "^10.1.3"
  },
  "files": [
    "CHANGELOG.md",
    "src/",
    "scripts/",
    "tools/",
    "*.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^20.9 || ^22.11"
  },
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "20m"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
