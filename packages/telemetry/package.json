{
  "name": "@agoric/telemetry",
  "version": "0.1.0",
  "description": "Agoric's telemetry implementation",
  "type": "module",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "main": "./src/index.js",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix && yarn lint:types",
    "lint-check": "yarn lint",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc -p jsconfig.json",
    "lint:eslint": "eslint '**/*.js'"
  },
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "dependencies": {
    "@opentelemetry/exporter-prometheus": "^0.16.0",
    "@opentelemetry/metrics": "^0.16.0"
  },
  "devDependencies": {
    "@endo/lockdown": "^0.1.1",
    "@endo/ses-ava": "^0.2.14",
    "ava": "^3.12.1",
    "c8": "^7.7.2"
  },
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=11.0"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "20m"
  }
}
