+    assertNumber(length);
+    if (length > 255 * hash.outputLen)
+        throw new Error('Length should be <= 255*HashLen');
+    const blocks = Math.ceil(length / hash.outputLen);
+    if (info === undefined)
+        info = EMPTY_BUFFER;
