{
  "agoricProposal": {
    "type": "/agoric.swingset.CoreEvalProposal"
  },
  "vendorPackages": [
    "@aglocal/portfolio-contract",
    "@aglocal/portfolio-deploy",
    "@agoric/client-utils",
    "@agoric/deploy-script-support",
    "@agoric/portfolio-api"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "dependencies": {
    "@aglocal/portfolio-contract": "portal:./local-packages/portfolio-contract",
    "@aglocal/portfolio-deploy": "portal:./local-packages/portfolio-deploy",
    "@agoric/deploy-script-support": "dev",
    "@agoric/synthetic-chain": "^0.6.1",
    "@endo/far": "^1.1.14",
    "@endo/init": "^1.1.12",
    "@endo/pass-style": "^1.6.3",
    "agoric": "dev",
    "ava": "^6.4.1"
  },
  "resolutions": {
    "@agoric/client-utils": "portal:./local-packages/client-utils",
    "@agoric/deploy-script-support": "portal:./local-packages/deploy-script-support",
    "@agoric/portfolio-api": "portal:./local-packages/portfolio-api",
    "@aglocal/portfolio-contract": "portal:./local-packages/portfolio-contract",
    "@aglocal/portfolio-deploy": "portal:./local-packages/portfolio-deploy"
  },
  "ava": {
    "concurrency": 1,
    "timeout": "2m",
    "files": [
      "test/**/*.test.*",
      "!submission"
    ],
    "extensions": {
      "js": true,
      "ts": "module"
    },
    "nodeArguments": [
      "--import=ts-blank-space/register",
      "--no-warnings"
    ]
  },
  "scripts": {
    "agops": "yarn --cwd /usr/src/agoric-sdk/ agops"
  },
  "packageManager": "yarn@4.12.0",
  "devDependencies": {
    "ts-blank-space": "^0.7.0"
  }
}
