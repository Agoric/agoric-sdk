{
  "name": "@agoric/fast-usdc",
  "version": "0.1.0",
  "description": "CLI and library for Fast USDC product",
  "type": "module",
  "files": [
    "contract",
    "scripts",
    "src",
    "tools"
  ],
  "main": "src/main.js",
  "bin": "./src/cli/bin.js",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 --all $C8_OPTIONS ava",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc",
    "lint:eslint": "eslint ."
  },
  "devDependencies": {
    "@agoric/deploy-script-support": "dev",
    "@agoric/eslint-config": "^0.4.1-dev-7efdf47.0",
    "@agoric/swingset-liveslots": "dev",
    "@agoric/vats": "dev",
    "@agoric/zoe": "dev",
    "@agoric/zone": "dev",
    "@fast-check/ava": "^2.0.1",
    "ava": "^5.3.0",
    "c8": "^10.1.2",
    "eslint": "^9.19.0",
    "execa": "9.1.0",
    "npm-run-all": "^4.1.5",
    "ts-blank-space": "^0.6.1",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@agoric/client-utils": "dev",
    "@agoric/cosmic-proto": "dev",
    "@agoric/ertp": "dev",
    "@agoric/fast-usdc-worker": "workspace:*",
    "@agoric/internal": "dev",
    "@agoric/notifier": "dev",
    "@agoric/orchestration": "dev",
    "@agoric/store": "dev",
    "@agoric/vat-data": "dev",
    "@agoric/vow": "dev",
    "@agoric/zoe": "dev",
    "@cosmjs/proto-signing": "^0.33.0",
    "@cosmjs/stargate": "^0.33.0",
    "@endo/base64": "^1.0.9",
    "@endo/common": "^1.2.9",
    "@endo/errors": "^1.2.9",
    "@endo/eventual-send": "^1.3.0",
    "@endo/far": "^1.1.10",
    "@endo/init": "^1.1.8",
    "@endo/marshal": "^1.6.3",
    "@endo/nat": "^5.0.14",
    "@endo/pass-style": "^1.4.8",
    "@endo/patterns": "^1.4.8",
    "@endo/promise-kit": "^1.1.9",
    "@nick134-bit/noblejs": "0.0.2",
    "bech32": "^2.0.0",
    "commander": "^12.1.0",
    "ethers": "^6.13.4"
  },
  "ava": {
    "extensions": {
      "js": true,
      "ts": "module"
    },
    "files": [
      "test/**/*.test.*"
    ],
    "nodeArguments": [
      "--import=ts-blank-space/register",
      "--no-warnings"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "20m"
  },
  "packageManager": "yarn@4.7.0",
  "license": "Apache-2.0",
  "resolutions": {
    "protobufjs@npm:^6.8.8": "patch:protobufjs@npm%3A6.11.4#~/.yarn/patches/protobufjs-npm-6.11.4-af11968b80.patch",
    "axios@npm:^1.6.0": "patch:axios@npm%3A1.8.1#~/.yarn/patches/axios-npm-1.8.1-8a5eaf948f.patch",
    "@agoric/boot": "portal:../../packages/boot"
  }
}
